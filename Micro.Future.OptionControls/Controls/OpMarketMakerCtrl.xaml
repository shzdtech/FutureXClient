                        </GridViewColumn>
                        <!--<GridViewColumn x:Name="PPosition" Header="PPosition" TextBlock.TextAlignment="Center" DisplayMemberBinding="{Binding PutOptionVM.Position}" HeaderContainerStyle="{StaticResource GreenHeaderStyle}"/>-->
