                                </DataTemplate>
                            </GridViewColumn.CellTemplate>
                        </GridViewColumn>
                        <!--<GridViewColumn x:Name="PTheta" Header="PTheta" DisplayMemberBinding="{Binding PutOptionVM.TheoDataVM.Theta365,StringFormat=N2}" HeaderContainerStyle="{StaticResource GreenHeaderStyle}"/>-->
                        <GridViewColumn x:Name="PTheta" Header="{lex:Loc Key=PTheta}" HeaderContainerStyle="{StaticResource GreenHeaderStyle}">
                            <GridViewColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding PutOptionVM.TheoDataVM.Theta365,StringFormat=N2}" TextAlignment="Center" Width="auto"/>
                                </DataTemplate>
